{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXTCLOUD_WEBDAV_URL": "https://seu-nextcloud.com/remote.php/webdav",
    "NEXTCLOUD_USER": "seu-usuario",
    "NEXTCLOUD_PASSWORD": "sua-senha"
  }
}
